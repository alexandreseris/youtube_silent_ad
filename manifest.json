{

    "manifest_version": 2,
    "name": "Youtube Silent Ad",
    "version": "2.0",
    "description": "Mute video ads on Youtube",
    "icons": {
        "48": "icons/icon.svg",
        "96": "icons/icon.svg"
    },
    "content_scripts": [
        {
            "matches": ["*://www.youtube.com/*"],
            "js": ["main.js"]
        }
    ]
}
